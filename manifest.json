{
    "manifest_version": 3,
    "name": "Cookie clicker hub",
    "description": "extension for cookie clicker, hub have only in-game scripts, ",
    "version": "1.0",
    "author": "Zengy",
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "content_scripts": [
      {
        "js": [
          "content-script.js"
        ],
        "matches": [
          "http://*.example.com//"
        ]
      }
    ]
  }